        fourtyfivekgs_entry = tk.Entry(FrameThree, font=('arial',13, 'bold'), bd = 8, width = 6,textvariable = variable_fourtyfivekgs_barbell, state = DISABLED)
        fourtyfivekgs_entry.grid(row =1, column = 1)
